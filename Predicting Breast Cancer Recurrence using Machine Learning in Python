        "from sklearn.preprocessing import LabelEncoder\n",
        "label_encoder = LabelEncoder()"
      ],
      "metadata": {
        "id": "iHb9AX069mDA"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# To encode the target train and test datasets\n",
        "y_train = label_encoder.fit_transform(y_train)\n",
        "y_test = label_encoder.transform(y_test)"
      ],
      "metadata": {
        "id": "Ls3_V49jNUfe"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# To get the columns with the categorical data from the features columns\n",
        "categorical_cols = X_train.select_dtypes(include=[\"object\", \"category\"]).columns.tolist()"
      ],
      "metadata": {
        "id": "5LTheeQSWKEu"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# To encode the features data in the train and test sets\n",
        "X_train = pd.get_dummies(X_train, columns=categorical_cols, drop_first=True, dtype=int)\n",
        "X_test = pd.get_dummies(X_test, columns=categorical_cols, drop_first=True, dtype=int)"
      ],
      "metadata": {
        "id": "h9zdVfzvWV8n"
      },
      "execution_count": null,
      "outputs": []
    },
