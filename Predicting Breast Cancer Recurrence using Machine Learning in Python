      ],
      "metadata": {
        "id": "oZQ8Ome8wokV"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "param_grid = {\n",
        "    \"C\" : [0.1, 1, 10],\n",
        "    \"kernel\" : [\"linear\", \"rbf\"],\n",
        "    \"gamma\" : [\"scale\", \"auto\"]\n",
        "}"
      ],
      "metadata": {
        "id": "psqhvC_Yw_Az"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "grid_search_precision = GridSearchCV(SVC(random_state=42, probability=True), param_grid, cv=5, scoring=\"precision\")\n",
        "grid_search_precision.fit(X_train_resampled, y_train_resampled)\n",
        "print(\"Best Parameters (Precision):\", grid_search_precision.best_params_)\n",
        "print(\"Best Precision:\", grid_search_precision.best_score_)"
      ],
