      "cell_type": "markdown",
      "source": [
        "7. Model Evalution"
      ],
      "metadata": {
        "id": "T_Hp5sSVzD8z"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import confusion_matrix, roc_auc_score"
      ],
      "metadata": {
        "id": "eTXIfhoxyjEr"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "y_pred_baseline = baseline_model.predict(X_test)\n",
        "tn, fp, fn , tp = confusion_matrix(y_test, y_pred_baseline).ravel() # tp= true positive, tn= true negative, fp = false positive, fn = false negative"
      ],
      "metadata": {
        "id": "QLebABvkzVhj",
        "collapsed": true
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# these are the metrics that the model will be evaluated on\n",
        "accuracy = (tp + tn) / (tp + tn + fp + fn)\n",
        "precision = tp / (tp + fp)\n",
        "recall = tp / (tp + fn)\n",
        "specificity = tn / (tn + fp)\n",
        "roc_auc = roc_auc_score(y_test, baseline_model.predict_proba(X_test)[:, 1])"
      ],
      "metadata": {
        "id": "N0b-v9wbzaZi",
        "collapsed": true
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "print(f\"Accuracy: {accuracy:.2f}\")\n",
        "print(f\"Precision:{precision:.2f}\")\n",
        "print(f\"Recall (Sensitivity): {recall:.2f}\")\n",
        "print(f\"Specificity: {specificity:.2f}\")\n",
        "print(f\"ROC_AUC Score:{roc_auc:.2f}\")"
      ],
      "metadata": {
        "id": "T3hTGjsg0Ktv",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "c208673a-92b1-4a9f-b5d9-16177830bd31"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Accuracy: 0.71\n",
            "Precision:0.50\n",
            "Recall (Sensitivity): 0.18\n",
            "Specificity: 0.93\n",
            "ROC_AUC Score:0.66\n"
          ]
        }
      ]
