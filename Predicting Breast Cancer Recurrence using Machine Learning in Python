      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.preprocessing import StandardScaler\n",
        "scaler = StandardScaler()"
      ],
      "metadata": {
        "collapsed": true,
        "id": "URtYihpUANw4"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# To get the numerical data from the features columns:\n",
        "num_features = X_train.select_dtypes(include=[\"int64\", \"float64\"]).columns.tolist()"
      ],
      "metadata": {
        "id": "2OqkLxATNk8y"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# To normalize the numerical data in the X_train and X_test\n",
        "X_train[num_features] = scaler.fit_transform(X_train[num_features])\n",
        "X_test[num_features] = scaler.transform (X_test[num_features])"
      ],
      "metadata": {
        "id": "If5Jq9n0ONnG"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "5. Feature Selection using Recursive Feature Elimination(RFE)"
      ],
      "metadata": {
        "id": "n0OUdpjvwiUR"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.feature_selection import RFE\n",
        "from sklearn.ensemble import RandomForestClassifier"
      ],
      "metadata": {
        "id": "yxcb_wP8wlzs"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model = RandomForestClassifier(random_state=42) # Use this to rank the features\n",
        "rfe = RFE(estimator=model, n_features_to_select=7) # selected 7 features as there are only 9 features in total\n",
        "X_train = rfe.fit_transform(X_train, y_train)\n",
        "X_test = rfe.transform(X_test)\n",
        "\n",
        "# the selected 7 features will be used to train the model"
      ],
      "metadata": {
        "id": "4Tsuskj4w3lv"
      },
      "execution_count": null,
      "outputs": []
